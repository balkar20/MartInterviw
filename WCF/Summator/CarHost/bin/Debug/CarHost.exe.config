<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="fileCar" value="D:\train\WCF\cars.xml"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="CarService.CarService" behaviorConfiguration="mexBehavior">
        <endpoint address="CarService" binding="basicHttpBinding" contract="CarService.ICarService"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8000/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>